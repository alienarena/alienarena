{	
	{
		map textures/dm-purgatory/bottom2_blendtex.tga		
		blendmap 3			
			400 textures/desert/cliffbottom.tga		
			200 textures/desert/rockground1.tga	
			200 textures/desert/pebbles.tga	
		blendnormalmap 2 
			0 textures/desert/cliffbottom_nm.tga
			1 textures/desert/rockground1_nm.tga	
	}
}
