bottom_texture
{	
	{
		map textures/dm-purgatory/bottom_blendtex.tga		
		blendmap 3	
			200 textures/texturelib/grass_0075.jpg		
			400 textures/texturelib/grass_0048.jpg					
			200 textures/desert/pebbles.tga	
		blendnormalmap 2 
			0 textures/texturelib/grass_0075_nm.tga	
			1 textures/texturelib/grass_0048_nm.tga
	
	}
}
