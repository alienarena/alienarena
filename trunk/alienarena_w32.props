<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <SrcDir>$(SolutionDir)source\</SrcDir>
    <BuildDir>$(SolutionDir)vs2010\</BuildDir>
    <LocalLibDir>$(BuildDir)lib\</LocalLibDir>
    <LocalIncludeDir>$(BuildDir)include\</LocalIncludeDir>
    <LocalBinDir>$(BuildDir)bin\</LocalBinDir>
    <LibSrcDir>$(BuildDir)libsrc\</LibSrcDir>
    <LibzlibSrcDir>$(LibSrcDir)zlib-1.2.5\</LibzlibSrcDir>
    <LibcurlSrcDir>$(LibSrcDir)curl-7.20.1\</LibcurlSrcDir>
    <LiboggSrcDir>$(LibSrcDir)libogg-1.2.0\</LiboggSrcDir>
    <LibvorbisSrcDir>$(LibSrcDir)libvorbis-1.3.1\</LibvorbisSrcDir>
    <LibjpegSrcDir>$(LibSrcDir)jpeg-8b\</LibjpegSrcDir>
    <LibodeSrcDir>$(LibSrcDir)ode-0.11.1\</LibodeSrcDir>
    <FreeTypeSrcDir>$(LibSrcDir)freetype-2.4.2\</FreeTypeSrcDir>
    <OpenALIncludeDir>C:\Program Files (x86)\OpenAL 1.1 SDK\include\</OpenALIncludeDir>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>alienarena_w32</_PropertySheetDisplayName>
    <OutDir>$(LocalLibDir)$(Configuration)\</OutDir>
    <IntDir>$(BuildDir)$(Configuration)\$(ProjectName)\</IntDir>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="SrcDir">
      <Value>$(SolutionDir)</Value>
    </BuildMacro>
    <BuildMacro Include="LocalLibDir">
      <Value>$(BuildDir)lib\</Value>
    </BuildMacro>
    <BuildMacro Include="LocalIncludeDir">
      <Value>$(BuildDir)include\</Value>
    </BuildMacro>
    <BuildMacro Include="LocalBinDir">
      <Value>$(BuildDir)bin\</Value>
    </BuildMacro>
    <BuildMacro Include="LibSrcDir">
      <Value>$(BuildDir)libsrc\</Value>
    </BuildMacro>
    <BuildMacro Include="LibzlibSrcDir">
      <Value>$(LibSrcDir)zlib-1.2.5\</Value>
    </BuildMacro>
    <BuildMacro Include="LibcurlSrcDir">
      <Value>$(LibSrcDir)curl-7.20.1\</Value>
    </BuildMacro>
    <BuildMacro Include="LiboggSrcDir">
      <Value>$(LibSrcDir)libogg-1.2.0\</Value>
    </BuildMacro>
    <BuildMacro Include="LibvorbisSrcDir">
      <Value>$(LibSrcDir)libvorbis-1.3.1\</Value>
    </BuildMacro>
    <BuildMacro Include="LibjpegSrcDir">
      <Value>$(LibSrcDir)jpeg-8b\</Value>
    </BuildMacro>
    <BuildMacro Include="LibodeSrcDir">
      <Value>$(LibodeSrcDir)</Value>
    </BuildMacro>
    <BuildMacro Include="FreeTypeSrcDir">
      <Value>$(FreeTypeSrcDir)</Value>
    </BuildMacro>
    <BuildMacro Include="OpenALIncludeDir">
      <Value>C:\Program Files (x86)\OpenAL 1.1 SDK\include\</Value>
    </BuildMacro>
  </ItemGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <FloatingPointModel>Fast</FloatingPointModel>
      <BasicRuntimeChecks>Default</BasicRuntimeChecks>
      <FunctionLevelLinking>true</FunctionLevelLinking>
      <StringPooling>true</StringPooling>
      <FloatingPointExceptions>false</FloatingPointExceptions>
      <CreateHotpatchableImage>false</CreateHotpatchableImage>
      <BufferSecurityCheck>false</BufferSecurityCheck>
      <WarningLevel>Level3</WarningLevel>
      <FavorSizeOrSpeed>Speed</FavorSizeOrSpeed>
      <IntrinsicFunctions>true</IntrinsicFunctions>
      <StructMemberAlignment>1Byte</StructMemberAlignment>
    </ClCompile>
    <Lib>
      <OutputFile>$(LocalLibDir)$(Configuration)\$(TargetName)$(TargetExt)</OutputFile>
    </Lib>
    <Link />
    <Link>
      <SubSystem>Windows</SubSystem>
    </Link>
  </ItemDefinitionGroup>
</Project>
